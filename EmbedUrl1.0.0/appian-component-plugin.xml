<?xml version="1.0" encoding="UTF-8"?>
<appian-component-plugin name="EmbedUrl" key="com.credicop.EmbedUrl">
  <plugin-info>
    <description>Provides a customizable embed landing on Appian</description>
    <vendor name="Credicorp" url="http://www.appian.com" />
    <support supported="true" email="jesus.pinto@vasscompany.com" url="https://community.appian.com"/>
    <version>1.0.0</version>
    <application-version min="17.3.0" max="24.2.392.0"/>
  </plugin-info>

  <component rule-name="EmbedUrl" version="1.0.0">
    <sdk-version>2.0.0</sdk-version>
    
    
    <parameter>
    <name>url</name>
    <category>input-only</category>
    <type>Text</type>
    </parameter>
    
    
    
    
    <html-entry-point>index.html</html-entry-point>
    <supported-user-agents>chrome firefox ie11 edge safari mobile</supported-user-agents>
  </component>
</appian-component-plugin>
